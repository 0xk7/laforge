include {
  path = "{{ js $.RelEnvPath }}"
}

include {
  path = "./teams/*/team.laforge"
}

build "{{ $.ID }}" {
  environment_id = "{{ $.EnvironmentID }}"
  competition_id = "{{ $.CompetitionID }}"
  revision = {{ $.Revision }}
  team_count = {{ $.TeamCount }}

  config = {
    {{ range $key, $val := $.Config -}}
    {{ $key }} = "{{ $val }}"
    {{ end -}}
  }

  tags = {
    {{ range $key, $val := $.Tags -}}
    {{ $key }} = "{{ $val }}"
    {{ end -}}
  }
}